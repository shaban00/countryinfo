{
  "name": "countryinfo",
  "version": "1.0.0",
  "description": "An API to get information about countries, states and cities",
  "main": "dist/app.js",
  "scripts": {
    "start": "node dist/app.js",
    "build": "npm run clean && babel src --out-dir dist --copy-files",
    "docker": "docker compose -f docker-compose.yml up -d --force-recreate --build",
    "docker:down": "docker compose -f docker-compose.yml down",
    "clean": "rm -rf dist/",
    "dev": "nodemon --exec babel-node src/app.js",
    "check": "prettier --check src/**/*.+(ts|tsx|scss)",
    "format": "prettier --write \"**/*.+(js|jsx|scss)\""
  },
  "keywords": [
    "Countries",
    "States",
    "Cities",
    "API",
    "Express.js",
    "Node.js"
  ],
  "author": "Shaban Mohammedsaani Hassan (shaban00)",
  "license": "MIT",
  "dependencies": {
    "body-parser": "^1.20.2",
    "cookie-parser": "^1.4.6",
    "core-js": "2",
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "helmet": "^7.0.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.22.15",
    "@babel/core": "^7.22.19",
    "@babel/node": "^7.22.19",
    "@babel/polyfill": "^7.12.1",
    "@babel/preset-env": "^7.22.15",
    "@babel/preset-react": "^7.22.15",
    "nodemon": "^3.0.1",
    "prettier": "^3.0.3"
  },
  "engines": {
    "node": ">=16.0.0"
  }
}
